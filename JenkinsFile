pipeline {
  agent any
  stages {
  stage('SCM Checkout'){
      steps{
        git url: 'https://github.com/bhushanphani/jenkins-cicd.git'
      }
  }
    stage('Deploy CloudHub') { 
      steps {
        def mvnHome= "tool name: 'local_maven', type: 'maven'"
        sh "${mvnHome}/bin/mvn clean install"
      }
    }
  }
}
